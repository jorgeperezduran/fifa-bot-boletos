{
    "exclude": [
        "**/*.spec.js","./dist"
    ],
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "outDir": "./dist",
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "types": ["jest", "node"],
        "strictNullChecks": false,
        "allowJs": true,
        "lib": ["dom", "esnext"],
        "moduleResolution": "node",
        "downlevelIteration": true,
        "jsx": "react",
        "resolveJsonModule": true,
    }
}